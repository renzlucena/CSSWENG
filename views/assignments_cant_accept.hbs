				<div class="main-header">
					<h1>Assignments</h1>
					<p><b><i>You have {{how_many_ongoing}} ongoing assignment(s)!</b></i></p>
					<p>Please confirm immediately to secure the job or it might be reassigned.
					If the assignments is not finished in one week, it will be reopened to all agents week, it will be reopened to all agents.</p>
				</div>

				<form method="get" action="/assignments">
					<div class="main-table">
						<table>
							<tr>
								<th>REF NO.</th>
								<th>DATE OF REQUEST</th>
								<th>CITY, REGION</th>
								<th>ACTION</th>
							</tr>			
							
							<tr>
								<th colspan='4'>New Assignments</th>
							</tr>
				
							{{#each assignment_new}}
								<tr>
									<td><p>{{this.ref_id}}</p></td>
									<td><p>{{this.created_at}}</p></td>
									<td><p>{{this.lot_brgy}}, {{this.lot_city}}, {{this.lot_region}}</p></td>
									<td>
										<div class="main-assignments-status-btns">
												<button disabled type="submit" name="accept" formaction="/accept-assignment/{{ref_id}}">Can't Accept</button>
										</div>
									</td>
								</tr>
							{{/each}}
							
							<tr>
								<th colspan='4'>Assigned Assignments</th>
							</tr>
						
							{{#each assignment_returned2}}
								<td><p>{{this.ref_id}}</p></td> 
								<td><p>{{this.created_at}}</p></td> 
								<td><p>{{this.lot_brgy}}, {{this.lot_city}}, {{this.lot_region}}</p></td> 
								<td> 
								<div class="main-assignments-status-btns"> 
									<button type="submit" formaction="/viewAssignment/0/{{ref_id}}">Edit Document</button> 
									</div> 
									<td> 
								</tr> 
								
								<tr>
									<td colspan='4'><p class="center-align">Sen</p></td>
								</tr>
							{{/each}}
							
							{{#each assignment_returned1}}
								<td><p>{{this.ref_id}}</p></td> 
								<td><p>{{this.created_at}}</p></td> 
								<td><p>{{this.lot_brgy}}, {{this.lot_city}}, {{this.lot_region}}</p></td> 
								<td> 
								<div class="main-assignments-status-btns"> 
									<button type="submit" formaction="/viewAssignment/0/{{ref_id}}">Edit Assignment</button> 
									</div> 
									<td> 
								</tr> 
								
								<tr>
									<th>Comment:</th>
									<td colspan='3'><p class="left-align">"{{this.comment}}"</p></td>
								</tr>
							{{/each}}
							
							<tr>
								<th colspan='4'>For Admin Review</th>
							</tr>
							
							{{#each assignment_sub}}
								<tr>
									<td><p>{{this.ref_id}}</p></td>
									<td><p>{{this.created_at}}</p></td>
									<td><p>{{this.lot_brgy}}, {{this.lot_city}}, {{this.lot_region}}</p></td>
									<td>
										<div class="main-assignments-status-btns">
											<button type="submit" formaction="/viewAssignment/0/{{ref_id}}">View Assignment</button>
										</div>
									</td>
								</tr>
								
								<tr>
									<td colspan='4'><p class="center-align">Sent to Admin for Review and Processing.</p></td>
								</tr>
							{{/each}}	
						
