                        <td><textarea id="legally_permissible" name="legally_permissible" value="{{this.legally_permissible}}" style="width:80%;" rows="10"></textarea></td>
                    </tr>
                    <tr>
                        <th><label for="physical_possibility">Physical Possibility</label></th>
                        <td><textarea id="physical_possibility" name="physical_possibility" value="{{this.physical_possibility}}" style="width:80%;" rows="10"></textarea></td>
                    </tr>
                    <tr>
                        <th><label for="financial_feasibility">Financial Feasibility</label></th>
                        <td><textarea id="financial_feasibility" name="financial_feasibility" value="{{this.financial_feasibility}}" style="width:80%;" rows="10"></textarea></td>
                    </tr>
