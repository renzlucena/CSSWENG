                    </tr>
                    <tr>
                        <th><label for="cost_value">Cost Approach Value</label></th>
                        <td><input type="text" id="cost_value" name="cost_value" value={{this.cost_value}}></td>
                    </tr>
                    <tr>
                        <th><label for="cost_value_per_sqm">Cost Approach Value per sqm</label></th>
                        <td><input type="text" id="cost_value_per_sqm" name="cost_value_per_sqm" value="{{this.cost_value_per_sqm}}"></td>
