                        <td><textarea id="market_analysis" name="market_analysis" value="{{this.market_analysis}}" style="width:80%;" rows="10"></textarea></td>
                    </tr>

                    <tr>
                        <th><label for="highest_best_use">Highest and Best Use</label></th>
                        <td><textarea id="highest_best_use" name="highest_best_use" value="{{this.highest_best_use}}" style="width:80%;" rows="10"></textarea></td>
                    </tr>
                    <tr>
